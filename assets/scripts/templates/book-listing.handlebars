{{#each books as |book|}}
  {{log book}}
  <!-- {{> partial}}
  Don't uncomment yet -->

  <div>
    <h1 data-id="{{book.id}}"> <small>Title: {{limit book.title 10}}</small></h1>
      <section data-id="{{book.id}}"
        <p>{{#if book.author}}
           By: {{book.author}}
          {{else}}
           No author provided.
          {{/if}}
        </p>
        <button type="submit" class="btn btn-default">Remove</button>
      </section>
  </div>

{{/each}}
